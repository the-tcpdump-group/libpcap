			(024) tax
			(025) ldh      [x + 0]
			(026) st       M[1]
			(027) ld       #0xabcd
			(028) st       M[2]
			(029) ldx      M[2]
			(030) ld       M[1]
			(031) jeq      x                jt 32	jf 33
			(032) ret      #65535
			(033) ret      #0
			',
	}, # udp_index_halfword
