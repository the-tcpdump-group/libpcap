		unopt => '
			(000) ld       [0]
			(001) and      #0xfffff000
			(002) jeq      #0x80211000      jt 3	jf 5
			(003) ld       [4]
			(004) ja       6
			(005) ld       #0x90
			(006) st       M[0]
			(007) tax
			(008) ldx      M[0]
			(009) ldb      [x + 1]
			(010) and      #0x3
			(011) jeq      #0x2             jt 12	jf 13
			(012) ret      #262144
			(013) ret      #0
			',
	}, # wlan_dir_fromds_PRISM_HEADER

