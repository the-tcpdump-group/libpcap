			(008) ldb      [0]
			(009) and      #0xf0
			(010) jeq      #0x60            jt 11	jf 33
			(011) ldb      [6]
			(012) jeq      #0x11            jt 17	jf 13
			(013) ldb      [6]
			(014) jeq      #0x2c            jt 15	jf 33
			(015) ldb      [40]
			(016) jeq      #0x11            jt 17	jf 33
			(017) ldh      [6]
			(018) jset     #0x1fff          jt 33	jf 19
			(019) ld       #0x8
			(020) st       M[0]
			(021) ldxb     4*([0]&0xf)
			(022) ld       M[0]
