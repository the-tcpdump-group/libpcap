			(016) ldh      [x + 16]
			(017) jge      #0x714           jt 18	jf 20
			(018) jgt      #0x714           jt 20	jf 19
			(019) ret      #262144
			(020) ret      #0
			',
	}, # dst_portrange_degenerate
