		name => 'wlan_type',
		DLT => 'IEEE802_11',
		# Type value out of range.
		expr => 'wlan type 16',
		errstr => 'invalid 802.11 type value',
	},
	{
		name => 'wlan_subtype',
		DLT => 'IEEE802_11',
		# Invalid syntax (numeric subtype is ambiguous and requires a type).
		expr => 'wlan subtype 0',
		errstr => errstr_syntax,
	},
	{
		name => 'wlan_type_subtype',
