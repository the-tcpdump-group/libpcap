			(184) ld       #0x97
			(185) st       M[11]
			(186) ldx      M[11]
			(187) ld       M[10]
			(188) jeq      x                jt 189	jf 204
			(189) ld       #0x98
			(190) st       M[11]
			(191) ld       #0x98
			(192) st       M[12]
			(193) ldx      M[12]
			(194) ld       M[11]
			(195) jeq      x                jt 196	jf 204
			(196) ld       #0x99
			(197) st       M[12]
			(198) ld       #0x99
			(199) st       M[13]
			(200) ldx      M[13]
			(201) ld       M[12]
			(202) jeq      x                jt 203	jf 204
			(203) ret      #262144
			(204) ret      #0
			',
	}, # icmp6_types

	{
		name => 'tcp_flags',
		DLT => 'EN10MB',
		aliases => ['
			0x01 == tcp-fin &&
			0x02 == tcp-syn &&
			0x04 == tcp-rst &&
			0x08 == tcp-push &&
			0x10 == tcp-ack &&
			0x20 == tcp-urg &&
			0x40 == tcp-ece &&
			0x80 == tcp-cwr
		'],
		opt => '
			(000) ret      #262144
			',
		unopt => '
			(000) ld       #0x1
			(001) st       M[0]
			(002) ld       #0x1
			(003) st       M[1]
			(004) ldx      M[1]
			(005) ld       M[0]
			(006) jeq      x                jt 7	jf 57
			(007) ld       #0x2
			(008) st       M[1]
			(009) ld       #0x2
			(010) st       M[2]
			(011) ldx      M[2]
			(012) ld       M[1]
			(013) jeq      x                jt 14	jf 57
